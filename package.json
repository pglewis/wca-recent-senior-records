{
    "name": "wca-recent-senior-records",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "homepage": "https://pglewis.github.io/wca-recent-senior-records/",
    "license": "GPL-2.0+",
    "engines": {
        "node": ">= 23.6.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.18.0",
        "@stylistic/eslint-plugin-js": "^2.12.1",
        "@types/node": "^22.15.18",
        "eslint": "^9.18.0",
        "eslint-plugin-jsdoc": "^50.6.1",
        "globals": "^15.14.0",
        "shx": "^0.4.0",
        "tsx-dom": "^3.1.0",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.20.0",
        "vite": "^6.0.5"
    },
    "scripts": {
        "dev": "npx vite",
        "lint": "npx eslint",
        "tsc-node": "npx tsc --project ./bin",
        "tsc-client": "npx tsc --project ./src",
        "tsc": "npm run tsc-node && npm run tsc-client",
        "build": "npm run lint && npm run tsc && npx vite build",
        "preview": "npx vite preview",
        "get-data": "node bin/get-data.ts",
        "copy-dev-data": "shx cp dist/data/* src/data/"
    }
}