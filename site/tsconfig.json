{
	// Client side config
	"extends": "../tsconfig.base.json",
	"compilerOptions": {
		"target": "ES2022",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"lib": [
			"ES2022",
			"DOM",
			"DOM.Iterable"
		],
		"noEmit": true,
		"jsx": "react",
		"jsxFactory": "h",
		"allowImportingTsExtensions": true,
		"isolatedModules": true,
		"skipLibCheck": true
	},
	"include": [
		"./**/*.ts",
		"./**/*.tsx", "../lib/types/kinch-types.ts",
	],
}